

<style type="text/css">
	/* Login */
	.fl {
		float: left;
		display: inline;
	}

	.loginBox {
		color: #62554c;
		height: 290px;
		z-index: 20;
		background: url(../images/logoBg.jpg) repeat-y;
		width: 262px;
		border: 1px solid #292d32;
		border-top: none;
		margin-top: -3px;
		float: left;
		position: relative;
	}

		.loginBox .form_login {
			color: #7d8ca0;
		}

	.logined .form_login {
		color: #7d8ca0;
		max-height: 160px;
		overflow-y: scroll;
		padding-bottom: 10px;
		margin-top: 10px;
	}

	.loginBox .form_int {
		width: 140px;
		padding: 19px 0 0 14px;
	}

	.loginBox .form_input {
		display: inline-block;
		float: left;
		width: 126px;
		height: 30px;
		color: #454e5b;
		border: none;
		padding-left: 25px;
		margin-top: 5px;
		line-height: 30px;
	}

		.loginBox .form_input:nth-child(1) {
			background: url(../images/Account.jpg) no-repeat;
		}

		.loginBox .form_input:nth-child(2) {
			background: url(../images/Password.jpg) no-repeat;
		}

	.loginBox .form_btn {
		padding: 24px 0 0 5px;
	}

	.loginBox .btn_login {
		width: 100px;
		height: 67px;
		background: url(../images/iconMap.png) no-repeat -230px -294px;
		border: none;
		cursor: pointer;
		text-indent: -9999em;
		display: block;
	}

		.loginBox .btn_login:hover {
			background-position: -362px -294px;
		}

	.loginBox .form_line {
		padding: 13px 14px 11px 15px;
		clear: both;
		font-size: 12px;
		color: #9aa8a6;
		font-family: "微软雅黑";
		text-align: right;
	}

		.loginBox .form_line input {
			vertical-align: middle;
			line-height: 20px;
			height: 20px;
			margin-right: 7px;
			float: left;
		}

		.loginBox .form_line label {
			float: left;
		}

	.loginBox .forgot_pw {
		color: #9aa8a6;
	}

	.loginBox .form_signup {
	}

		.loginBox .form_signup .btn_signup {
			display: block;
			margin: 0 auto;
			width: 232px;
			height: 53px;
			background: url(../images/iconMap.png) no-repeat -230px -389px;
			border: none;
			cursor: pointer;
			text-indent: -9999em;
		}

			.loginBox .form_signup .btn_signup:hover {
				background-position: -230px -460px;
			}

	.loginBox .quick_login {
		padding: 15px 15px 0 14px;
		font-size: 12px;
		color: #fff;
		font-family: "微软雅黑";
	}

		.loginBox .quick_login a {
			display: inline-block;
			width: 20px;
			height: 20px;
			background: url(../images/iconMap.png) no-repeat;
			vertical-align: middle;
			margin-left: 10px;
			text-indent: -9999em;
		}

			.loginBox .quick_login a.icon_facebook {
				background-position: -360px -223px;
			}

				.loginBox .quick_login a.icon_facebook:hover {
					background-position: -360px -253px;
				}

			.loginBox .quick_login a.icon_google {
				background-position: -385px -223px;
			}

				.loginBox .quick_login a.icon_google:hover {
					background-position: -385px -253px;
				}

			.loginBox .quick_login a.icon_twitter {
				background-position: -416px -223px;
			}

				.loginBox .quick_login a.icon_twitter:hover {
					background-position: -416px -253px;
				}

			.loginBox .quick_login a.icon_yahoo {
				background-position: -447px -223px;
			}

				.loginBox .quick_login a.icon_yahoo:hover {
					background-position: -447px -253px;
				}

			.loginBox .quick_login a.icon_windows {
				background-position: -475px -223px;
			}

				.loginBox .quick_login a.icon_windows:hover {
					background-position: -475px -253px;
				}
